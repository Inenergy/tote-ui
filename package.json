{
  "name": "tote-ui",
  "version": "0.2.0",
  "description": "GUI for micro tube fuel cell demo",
  "main": "src/main.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/SonikDropout/tote-ui"
  },
  "scripts": {
    "wp-dev": "webpack --watch",
    "wp-build": "webpack",
    "dev": "run-p wp-dev electron-dev",
    "electron-dev": "cross-env NODE_ENV=development electron .",
    "electron-test": "cross-env NODE_ENV=test electron .",
    "el-build": "electron-builder --dir --linux --armv7l",
    "build": "run-s wp-build el-build",
    "test": "jest",
    "postinstall": "electron-builder install-app-deps"
  },
  "keywords": [
    "car",
    "education",
    "fuel cells"
  ],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "chokidar": "^3.0.0",
    "cross-env": "^6.0.3",
    "css-loader": "^3.4.2",
    "electron": "^6.0.0",
    "electron-builder": "^21.2.0",
    "file-loader": "^5.0.2",
    "jest": "^25.1.0",
    "jest-esm-transformer": "^1.0.0",
    "mini-css-extract-plugin": "^0.10.0",
    "npm-run-all": "^4.1.5",
    "style-loader": "^1.1.3",
    "svelte": "^3.17.0",
    "svelte-loader": "^2.13.6",
    "url-loader": "^3.0.0",
    "webpack": "^4.41.5",
    "webpack-cli": "^3.3.10"
  },
  "jest": {
    "transform": {
      ".mjs": "jest-esm-transformer"
    },
    "moduleFileExtensions": [
      "js",
      "mjs",
      "json"
    ]
  },
  "prettier": {
    "svelteSortOrder": "scripts-markup-styles",
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "dependencies": {
    "chart.js": "^2.9.3",
    "chartjs-plugin-zoom": "^0.7.5",
    "serialport": "^9.0.0",
    "usb-detection": "^4.9.0"
  },
  "build": {
    "directories": {},
    "linux": {
      "executableName": "tote-ui",
      "category": "education"
    }
  }
}
